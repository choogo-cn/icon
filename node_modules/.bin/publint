#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/publint@0.1.12/node_modules/publint/lib/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/publint@0.1.12/node_modules/publint/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/publint@0.1.12/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/publint@0.1.12/node_modules/publint/lib/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/publint@0.1.12/node_modules/publint/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/publint@0.1.12/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/publint@0.1.12/node_modules/publint/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/publint@0.1.12/node_modules/publint/lib/cli.js" "$@"
fi
