#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules/@sveltejs/package/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules/@sveltejs/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules/@sveltejs/package/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules/@sveltejs/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules:/Users/jarvis/workspace/choogo.cn/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules/@sveltejs/package/svelte-package.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@sveltejs+package@2.0.2_svelte@3.59.1_typescript@5.0.4/node_modules/@sveltejs/package/svelte-package.js" "$@"
fi
